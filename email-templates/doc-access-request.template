  <body
    style="
      margin: 0;
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
      font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto',
        'Oxygen', 'Ubuntu', 'Cantarell', 'Fira Sans', 'Droid Sans',
        'Helvetica Neue', sans-serif;
      background: #ffffff;
      color: #333;
    "
  >
    <div
      class="content"
      style="
        margin-top: 2rem;
        padding: 2rem;
        display: flex;
        justify-content: center;
      "
    >
      <table
        role="presentation"
        align="center"
        style="border-spacing: 0; max-width: 600px; width: 600px"
      >
        <thead>
          <tr>
            <td
              align="right"
              style="border-collapse: collapse; padding-bottom: 2rem"
            >
              <img
                class="logo"
                src="https://developer.fiserv.com/assets/img/devstudio-logo.png"
                alt="Fiserv"
                width="376"
              />
            </td>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td style="border-collapse: collapse">
              <p
                style="
                  margin: 0;
                  padding: 0;
                  font-size: 16px;
                  line-height: 1.5rem;
                  padding: 0 0 20px 0;
                "
              >
                Hi there,
              </p>
            </td>
          </tr>

          <tr>
            <td style="height: 10px; font-size: 1px; line-height: 10px">
              &nbsp;
            </td>
          </tr>

          <tr>
            <td>
              <p
                style="
                  margin: 0;
                  padding: 0;
                  font-size: 16px;
                  line-height: 1.5rem;
                  padding: 0 0 20px 0;
                "
              >
                <b>{{.UserName}}</b> has requested access to a document,<br />please
                see the details below:
              </p>
            </td>
          </tr>

          <tr>
            <td style="height: 10px; font-size: 1px; line-height: 10px">
              &nbsp;
            </td>
          </tr>

          <tr>
            <td style="border-collapse: collapse">
              <p
                style="
                  margin: 0;
                  padding: 0;
                  font-size: 16px;
                  line-height: 1.5rem;
                  padding: 0 0 10px 0;
                "
              >
                <strong>Request Email:</strong> {{.EmailName}}
              </p>
            </td>
          </tr>

          <tr>
            <td style="height: 10px; font-size: 1px; line-height: 5px">
              &nbsp;
            </td>
          </tr>

          <tr>
            <td>
              <p
                style="
                  margin: 0;
                  padding: 0;
                  font-size: 16px;
                  line-height: 1.5rem;
                  padding: 0 0 10px 0;
