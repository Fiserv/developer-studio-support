        <tfoot>
          <tr>
            <td style="height: 10px; font-size: 1px; line-height: 10px">
              &nbsp;
            </td>
          </tr>
          <tr>
            <td style="border-collapse: collapse; padding: 0">
              <p
                class="footer"
                style="
                  margin: 0;
                  padding: 0;
                  font-size: 10px;
